page {
  height: 100%;
}
/* pages/content/content.wxss */
.md {
  width: 100%;
  margin-bottom: 60rpx;
}
.md .type_tea {
  font-size: 15px;
  color: mediumseagreen;
  font-weight: 3px;
  padding-left: 30rpx;
  border-left: 3px solid green;
}
.md .Tea {
  width: 100%;
  display: flex;
  /* flex-direction: row;
flex-wrap: true; */
  flex-flow: row wrap;
}
.md .Tea .Tea_img {
  width: 40%;
  height: 210rpx;
  margin-bottom: 40rpx;
  margin-left: 6%;
  margin-top: 30rpx;
  text-align: center;
  font-size: 25rpx;
}
.md .Tea .Tea_img .img {
  width: 100%;
  height: 100%;
}
.md .Tea .Tea_img view .tfname {
  color: #36c37d;
}
.md .Tea .Tea_img view .money {
  color: red;
  padding-left: 25rpx;
}
/* pages/scrollnav/scrollnav.wxss */
.tui-fixed-x {
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
}
/* .md{
  height: 100%;
  width:100%;
} */
.tui-city-scroll {
  height: 100%;
  line-height: 50rpx;
  width: 100%;
  white-space: nowrap;
}
.df {
  width: 100%;
  display: flex;
}
.tui-nav-li {
  flex: 1;
  text-align: center;
  height: 50rpx;
  line-height: 50rpx;
  width: 33.33%;
  box-sizing: border-box;
  font-size: 33rpx;
  padding: 0 10rpx;
}
.tui-nav-active {
  color: red;
  border-bottom: 3rpx solid red;
}
.tui-fixed-y {
  width: 100%;
  height: calc(20%);
}
.tui-city-scroll-y {
  display: flex;
  height: 100%;
  box-sizing: border-box;
}
