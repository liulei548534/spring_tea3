.box {
  width: 750rpx;
  height: 1136rpx;
}
.box .box_score {
  padding: 40rpx 20rpx;
  height: 260rpx;
  background-color: #f4f4f4;
}
.box .box_score .score_left {
  float: left;
  width: 200rpx;
  height: 250rpx;
  border: 2px solid #e1e1e1;
  background-color: #fff;
}
.box .box_score .score_left .left_score_num {
  color: #f40;
}
.box .box_score .score_left .left_score_num view {
  margin: 20rpx auto;
  font-size: 60rpx;
  text-align: center;
  width: 160rpx;
  border-bottom: 1rpx solid #e6e1e1;
}
.box .box_score .score_left .left_score_count view {
  color: #999;
  margin: 20rpx auto;
  font-size: 30rpx;
  text-align: center;
  width: 160rpx;
}
.box .box_score .score_left .left_score_star {
  padding: 0 20rpx;
}
.box .box_score .score_left .left_score_star text {
  font-size: 30rpx;
  letter-spacing: 5rpx;
  color: #f40;
}
.box .box_score .score_right {
  float: left;
  margin-left: 40rpx;
  margin-top: -10rpx;
}
.box .box_score .score_right .countScore {
  margin: 5rpx 0;
}
.box .box_score .score_right .countScore .star {
  margin-right: 20rpx;
}
.box .box_score .score_right .countScore .degree {
  margin-top: 10rpx;
  float: right;
  width: 350rpx;
  height: 30rpx;
}
.box .box_score .score_right .countScore .degree text {
  margin-left: 50rpx;
}
.box .box_content .content_both {
  letter-spacing: 5rpx;
  text-align: center;
  margin: 20rpx auto;
  border-bottom: 1px solid #46c47f;
}
.box .box_content .content_nav .content_nav_head {
  position: relative;
}
.box .box_content .content_nav .content_nav_head text {
  position: absolute;
  top: 15rpx;
  display: inline-block;
  line-height: 25rpx;
  font-size: 30rpx;
  margin-left: 20rpx;
}
.box .box_content .content_nav .content_nav_head image {
  margin: 0 0 0 20rpx;
  box-sizing: border-box;
  width: 54rpx;
}
.box .box_content .content_nav .content_nav_head .nickName {
  color: #999;
}
.box .box_content .content_nav .head_data .left_score_star {
  padding: 0 20rpx;
  position: relative;
}
.box .box_content .content_nav .head_data .left_score_star .icon {
  font-size: 30rpx;
  letter-spacing: 5rpx;
  color: #f40;
}
.box .box_content .content_nav .head_data .left_score_star .date {
  position: absolute;
  right: 4%;
  color: #999;
  font-size: 24rpx;
}
.box .box_content .content_nav .context_text {
  margin: 10rpx 20rpx;
  font-size: 30rpx;
  border-bottom: 1px solid #999;
}
.box .box_content .content_nav .context_text .context_answer {
  margin: 20rpx 0;
  width: 98%;
  height: 200rpx;
  background-color: #f4f4f4;
}
.box .box_content .content_nav .context_text .context_answer .s_name {
  height: 70rpx;
  position: relative;
  margin: 20rpx;
  border-bottom: 1px solid #e1e1e1;
}
.box .box_content .content_nav .context_text .context_answer .s_name .answer {
  line-height: 70rpx;
  color: #36c37d;
  font-size: 30rpx;
  font-weight: bold;
}
.box .box_content .content_nav .context_text .context_answer .s_name .date {
  line-height: 70rpx;
  position: absolute;
  right: 2%;
  color: #999;
  font-size: 24rpx;
}
.box .box_content .content_nav .context_text .context_answer .content {
  margin: 20rpx;
}
.box .box_content .content_nav .context_text .neirong {
  margin-right: 10rpx;
}
