page {
  width: 100%;
  height: 100%;
  background-color: #e1e1e1;
}
.search_head {
  display: flex;
  padding: 20rpx;
  margin-bottom: 20rpx;
}
.search_head .left {
  flex: 3;
}
.search_head .left input {
  width: 100%;
  border: 1px solid #f5f5f5;
  border-radius: 10rpx;
  background-color: white;
}
.search_head .right {
  border: 1px solid #f5f5f5;
  border-radius: 10rpx;
  text-align: center;
  width: 100%;
  margin: 0 10rpx 0 40rpx;
  flex: 1;
  background-color: #fff;
}
.content {
  box-sizing: border-box;
  padding: 0 10rpx;
  line-height: 60rpx;
  height: 60rpx;
  background-color: #fff;
  margin: 20rpx;
}
